{
  "name": "dusk-ui-kit",
  "version": "1.0.4-alpha",
  "description": "Dusk Web Kit",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "fix": "yarn fix:eslint",
    "start": "node server.dev.js",
    "dev": "NODE_ENV=development PORT=5000 node server.dev.js",
    "test": "jest",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "build:babel": "babel src/components --out-dir pkg --ignore 'src/**/*.stories.js','src/**/*.test.js','src/**/__snapshots__','src/**/*.flow'",
    "build:sass": "node-sass src/styles/index.scss --include-path node_modules -o pkg/styles --output-style compressed",
    "prepublish": "yarn run build:babel && yarn run build:sass",
    "design-system": "yarn test:generate-output && start-storybook",
    "prebuild:design-system": "yarn test:generate-output",
    "build:design-system": "build-storybook -s public",
    "predeploy": "yarn build:design-system"
  },
  "files": [
    "pkg/atoms/**/*",
    "pkg/molecules/**/*",
    "pkg/organisms/**/*",
    "pkg/templates/**/*",
    "pkg/styles/index.css"
  ],
  "keywords": [
    "dusk",
    "dusk network",
    "dusk ui kit",
    "dusk components"
  ],
  "author": "Kieran Hall <kieran@dusk.network>",
  "bugs": {
    "url": "https://github.com/dusk-network/dusk-ui-kit/issues"
  },
  "homepage": "https://github.com/dusk-network/dusk-ui-kit#readme",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@emotion/core": "^10.0.27",
    "@sambego/storybook-state": "^1.3.6",
    "@storybook/addon-a11y": "^5.3.0-beta.31",
    "@storybook/addon-actions": "^5.3.0-beta.31",
    "@storybook/addon-backgrounds": "^5.3.0-beta.31",
    "@storybook/addon-docs": "^5.3.0-beta.31",
    "@storybook/addon-jest": "^5.3.0-beta.31",
    "@storybook/addon-notes": "^5.3.0-beta.31",
    "@storybook/addon-viewport": "^5.3.0-beta.31",
    "@storybook/addons": "^5.3.0-beta.31",
    "@storybook/components": "^5.3.0-beta.31",
    "@storybook/react": "^5.3.0-beta.31",
    "@storybook/theming": "^5.3.0-beta.31",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "emotion": "^10.0.27",
    "express": "^4.16.0",
    "express-static-gzip": "^0.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-bootstrap": "latest",
    "react-dom": "^16.12.0",
    "react-toggle": "^4.1.1",
    "react-toggle-button": "^2.2.0",
    "sass-extract": "^2.1.0",
    "storybook-addon-xd-designs": "^5.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-transform-react-inline-elements": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-flow": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@svgr/webpack": "^4.3.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "css-loader": "^3.2.1",
    "css-modules-flow-types-loader": "^1.3.0",
    "empty": "^0.10.1",
    "eslint": "^4.19.1",
    "eslint-config-cheesecakelabs": "^2.0.3",
    "eslint-plugin-flowtype": "^2.39.1",
    "file-loader": "^1.1.5",
    "flow-bin": "^0.112.0",
    "jest": "^24.9.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "react-is": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "rfs": "^9.0.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.9.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>__mocks__/file.js",
      "\\.(css|less|scss|sass)$": "<rootDir>__mocks__/style.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/storybook-static/",
      "<rootDir>/pkg/",
      "<rootDir>/src/styles/",
      "<rootDir>/src/documentation/",
      "<rootDir>/__mocks__/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dusk-network/dusk-ui-kit.git"
  }
}
